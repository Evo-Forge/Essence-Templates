{
  "name": "essence-templates",
  "version": "1.0.8",
  "description": "React Essence Templates - based on Essence Material Design Framework",
  "homepage": "http://getessence.io/",
  "repository": "git://github.com/Evo-Forge/Essence-Templates",
  "author": "Hetmann W. Iohan <iohan@getessece.io>",
  "license": "MIT",
  "scripts": {
    "template:dev": "webpack-dev-server --devtool eval --hot --progress --colors --optimize-minimize --config essence/js/webpack.config.js --port 8080 --host 0.0.0.0 --content-base essence/",
    "template:essence": "webpack -p --optimize-minimize --config essence/js/webpack.config.js && http-server ./essence/ -p 8080 -a 0.0.0.0 -o",
    "template:custom": "webpack-dev-server --devtool eval --hot --progress --colors --config custom/js/webpack.config.js --port 8080 --host 0.0.0.0 --content-base custom/"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "keywords": [
    "essence",
    "react",
    "react-essence",
    "essence-templates",
    "react-component",
    "google",
    "google-material",
    "google-material-design",
    "basic",
    "frontend",
    "ui",
    "framework",
    "less"
  ],
  "dependencies": {
    "react-essence": "^1.x"
  },
  "devDependencies": {
    "essence-appbar": "^1.x",
    "essence-bottomsheet": "^1.x",
    "essence-button": "^1.x",
    "essence-card": "^1.x",
    "essence-chip": "^1.x",
    "essence-core": "^1.x",
    "essence-data-table": "^1.x",
    "essence-dialog": "^1.x",
    "essence-icon": "^1.x",
    "essence-image": "^1.x",
    "essence-input": "^1.x",
    "essence-list": "^1.x",
    "essence-menu": "^1.x",
    "essence-navigation": "^1.x",
    "essence-paper": "^1.x",
    "essence-progress": "^1.x",
    "essence-slider": "^1.x",
    "essence-snackbar": "^1.x",
    "essence-stepper": "^1.x",
    "essence-switch": "^1.x",
    "essence-tab": "^1.x",
    "essence-toast": "^1.x",
    "essence-tooltip": "^1.x",
    "essence-touchpad": "^1.x",
    "babel-core": "^6.3.x",
    "babel-loader": "^6.2.x",
    "babel-preset-es2015": "^6.3.x",
    "babel-preset-react": "^6.3.x",
    "css-loader": "^0.23.x",
    "extract-text-webpack-plugin": "^0.9.x",
    "gulp-less": "^3.0.x",
    "file-loader": "^0.8.x",
    "less": "^2.5.x",
    "less-loader": "^2.2.x",
    "lodash": "^4.3.0",
    "classnames": "^2.2.x",
    "react": "^0.14.x",
    "react-dom": "^0.14.x",
    "react-addons-css-transition-group": "^0.14.x",
    "react-addons-transition-group": "^0.14.x",
    "react-addons-update": "^0.14.x",
    "react-hot-loader": "^1.3.x",
    "style-loader": "^0.13.x",
    "url-loader": "^0.5.x",
    "webpack": "^1.12.x",
    "webpack-dev-server": "^1.14.x"
  }
}
